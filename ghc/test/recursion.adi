let fibonacci = mu f.λn.

    if (n - 1) then 
        if (n - 2) then 
            ((f (n - 1)) + (f (n - 2))) 
        else 1
    else 1 
;
let factorial = mu f.λn.

    if (n - 1) then
        (n * (f (n - 1)))
    else 1
;

let join = λa.λf.λb.(f (("" + a) + (", " + b)));
let tail = fn x.("" + x);
(
    join (fibonacci  4) 
    join (fibonacci  6)
    join (fibonacci  8) 
    join (factorial  7) 
    join (factorial  6)
    tail (factorial  1)
)
